ps/recipeForm\",\n        query: {\n          groupId: this.$route.params.groupId,\n          creatorId,\n          recipeId\n        }\n      });\n    },\n    removeRecipe(recipeId) {\n      this.$store\n        .dispatch(\"removeRecipeFromGroup\", {\n          recipeId,\n          groupId: this.$route.params.groupId\n        })\n        .then(() => this.$toast.Success(\"Recipe Deleted Successfully\"));\n    },\n    uploadRecipe() {}\n  },\n  computed: {\n    isGroupAdmin() {\n      return this.$store.getters.isGroupAdmin;\n    }\n  },\n  created() {}\n};\n</script>\n\n<style scoped lang=\"scss\">\n@media only screen and (min-width: 600px) {\n  ul,\n  ol {\n    padding-top: 5vh;\n    li > .quantity {\n      font-size: 1.3em !important;\n    }\n    li {\n      list-style: unset;\n    }\n  }\n  h1 {\n    margin: 15px 0;\n    font-size: 1.75em;\n  }\n  .accordion {\n    margin: 0 50px;\n    .recipes-header {\n      display: flex;\n      align-items: center;\n      h1 {\n        color: #607d8b;\n        font-size: 30px;\n\n        margin-right: 5px;\n      }\n    }\n  }\n  .el-carousel {\n    width: 250px;\n    height: 200px;\n  }\n  .el-carousel__item {\n    width: 250px;\n    height: 200px;\n    img {\n      width: 100%;\n      height: 100%;\n    }\n  }\n  .el-carousel__item:nth-child(2n) {\n    background-color: #99a9bf;\n  }\n  .el-carousel__item:nth-child(2n + 1) {\n    background-color: #d3dce6;\n  }\n  .collapse-context {\n    padding: 30px;\n    display: flex;\n    justify-content: space-between;\n  }\n  .el-collapse-item__header {\n  }\n  i {\n    margin-left: 5px;\n    font-size: 2em;\n    &:hover {\n      color: lightseagreen;\n      cursor: pointer;\n    }\n  }\n}\n\n@media only screen and (max-width: 600px) {\n  ul,\n  ol {\n    padding-top: 5vh;\n    li > .quantity {\n      font-size: 1.3em !important;\n    }\n    li {\n      list-style: unset;\n    }\n  }\n  h1 {\n    margin: 15px 0;\n    font-size: 1.75em;\n  }\n  .accordion {\n    margin: 0 50px;\n    